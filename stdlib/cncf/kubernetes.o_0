{{{The templates used to instantiate frames for data returned from Kubernetes.}}}
kubernetes : {
	{{{Details of the condition for a variety of different entities.}}}
	condition_tmpl : Template {
		{{{Human-readable message indicating details about last transition.}}}
		message : Required
		{{{Unique, one-word, camel-case reason for the condition's last transition.}}}
		reason : Required
		{{{Status is the status of the condition. Can be true, false, or null (unknown).}}}
		status : Required
		{{{Type is the type of the condition.}}}
		type : Required
  }
	{{{A configuration map which contains non-secret configuration data.}}}
	config_map_tmpl : Template {
		{{{A key-value map of configuration data; values may be strings or binary data.}}}
		data : Required
		{{{The name of this config map.}}}
		name : Required
		{{{The namespace to which this config map belongs.}}}
		namespace : Required
	}
	{{{A daemon set ensures that all (or some) nodes run a copy of a pod. As nodes are added to the cluster, pods are added to them.}}}
	daemon_set_tmpl : Template {
		{{{Represents the latest available observations of a daemon set's current state.}}}
		conditions : Required
		{{{The minimum number of seconds for which a newly created daemon set pod should be ready without any of its container crashing, for it to be considered available.}}}
		min_ready_seconds : Required
		{{{The maximum number of pods that can be unavailable during the update. Value can be an absolute number (\Emph{e.g.}, 5) or a percentage of total number of pods at the start of the update (\Emph{e.g.}, 10%).}}}
		max_unavailable : Required
		name : Required
		namespace : Required
		{{{The number of old history to retain to allow rollback.}}}
		revision_history_limit : Required
		{{{A label query over pods that are managed by the daemon set.}}}
		selector : Required
		{{{A description of the pod that will be created.}}}
		template : Required
		{{{Type of daemon set update.}}}
		update_stragety : Required
		{{{Count of hash collisions for the daemon set.}}}
		status_collision_count : Required
		{{{The number of nodes that are running at least 1 daemon pod and are supposed to run the daemon pod.}}}
		status_current_number_scheduled : Required
		{{{The total number of nodes that should be running the daemon pod (including nodes correctly running the daemon pod).}}}
		status_desired_number_scheduled : Required
		{{{The number of nodes that should be running the daemon pod and have one or more of the daemon pod running and available (ready for at least \{min_ready_seconds}).}}}
		status_number_available : Required
		{{{The number of nodes that are running the daemon pod, but are not supposed to run the daemon pod.}}}
		status_number_misscheduled : Required
		{{{The number of nodes that should be running the daemon pod and have one or more of the daemon pod running and ready.}}}
		status_number_ready : Required
		{{{The number of nodes that should be running the daemon pod and have none of the daemon pod running and available (ready for at least \{minReadySeconds}).}}}
		status_number_unavailable : Required
		{{{The most recent generation observed by the daemon set controller.}}}
		status_observed_generation : Required
		{{{The total number of nodes that are running updated daemon pod.}}}
		status_updated_number_scheduled : Required
	}
	deployment_tmpl : Template {
		conditions : Required
		min_ready_seconds : Required
		name : Required
		namespace : Required
		paused : Required
		process_deadline_seconds : Required
		replicas : Required
		revision_history_limit : Required
		revision_history_limit : Required
		status_available_replicas : Required
		status_collision_count : Required
		status_observed_generation : Required
		status_ready_replicas : Required
		status_replicas : Required
		status_unavailable_replicas : Required
		status_updated_replicas : Required
	}
	egress_policy_tmpl : Template {
		{{{A list of peers; each is a frame with \{namespace_selector} and \{pod_selector} which are label selectors.}}}
		peers : Required
		{{{A list of ports that are open; each is a frame with \{protocol} and \{port}.}}}
		ports : Required
	}
	endpoint_tmpl : Template {
		name : Required
		namespace : Required
		{{{ \{subset_tmpl} }}}
		subsets : Required
	}
	horizontal_pod_auto_scalers_tmpl : Template {
		conditions : Required
		max_replicas : Required
		min_replicas : Required
		name : Required
		namespace : Required
		status_current_replicas : Required
		status_desired_replicas : Required
		status_last_scale_time : Required
		status_observed_generation : Required
	}
	ingress_policy_tmpl : Template {
		{{{A list of peers; each is a frame with \{namespace_selector} and \{pod_selector} which are label selectors.}}}
		peers : Required
		{{{A list of ports that are open; each is a frame with \{protocol} and \{port}.}}}
		ports : Required
	}
	limit_range_tmpl : Template {
		name : Required
		namespace : Required
		type : Required
	}
	{{{The template for a namespace in Kubernetes.}}}
	namespace_tmpl : Template {
		{{{The name of the namespace.}}}
		namespace : Required
		{{{The current phase of the namespace.}}}
		phase : Required
	}
	network_policies_tmpl : Template {
		{{{ \{kubernetes.egress_policy_tmpl} }}}
		egress : Required
		{{{ \{kubernetes.ingress_policy_tmpl} }}}
		ingress : Required
		name : Required
		namespace : Required
		policy_types : Required
	}
	node_tmpl : Template {
	}
	persistent_volume_tmpl : Template {
		access_modes : Required
		name : Required
		namespace : Required
		status_message : Required
		status_reason : Required
	}
	{{{The template for a pod in Kubernetes.}}}
	pod_tmpl : Template {
		{{{The name of the pod.}}}
		name : Required
		{{{The namespace to which the pod belongs.}}}
		namespace : Required
		{{{A human-readable message explaining why the pod is in this state.}}}
		status_message : Required
		{{{A machine-readable explanation why the pod is in this state.}}}
		status_reason : Required
		{{{The current phase of the pod. \Link{https://kubernetes.io/docs/concepts/workloads/pods/pod-lifecycle/#pod-phase}}}}
		phase : Required
		{{{The time the pod was started.}}}
		start_time : Required
	}
	pod_spec_tmpl : Template {
		{{{Optional duration in seconds the pod may be active on the node relative to start time before the system will actively try to mark it failed and kill associated containers.}}}
		active_deadliene_seconds : Required
		{{{If specified, the pod's scheduling constraints.}}}
		affinity : Required
		{{{Whether a service account token should be automatically mounted.}}}
		automount_service_account_token : Required
		{{{List of containers belonging to the pod.}}}
	}
	priority_class_tmpl : Template {
		description : Required
		global_default : Required
		name : Required
		namespace : Required
		value : Required
	}
	replica_set_tmpl : Template {
		conditions : Required
		min_ready_seconds : Required
		name : Required
		namespace : Required
		replicas : Required
		status_available_replicas : Required
		status_fully_labelled_replicas : Required
		status_observed_generation : Required
		status_ready_replicas : Required
		status_replicas : Required
	}
	replication_controller_tmpl : Template {
		conditions : Required
		min_ready_seconds : Required
		name : Required
		namespace : Required
		replicas : Required
		status_available_replicas : Required
		status_fully_labelled_replicas : Required
		status_observed_generation : Required
		status_ready_replicas : Required
		status_replicas : Required
	}
	resource_quota_tmpl : Template {
		hard : Required
		name : Required
		namespace : Required
		scopes : Required
		status_hard : Required
		status_used : Required
	}
	role_binding_tmpl : Template {
    is_cluster : Required
    name : Required
    namespace : Required
    role_api_group : Required
    role_kind : Required
    role_name : Required
		{{{ \{subject_tmpl} }}}
    subjects : Required
	}
	role_tmpl : Template {
		name : Required
		namespace : Required
		is_cluster : Required
		{{{ \{rule_tmpl} }}}
		rules : Required
	}
	rule_tmpl : Template {
		api_groups : Required
		non_resource_urls : Required
		resource_names : Required
		resources : Required
		verbs : Required
	}
	secret_tmpl : Template {
		name : Required
		namespace : Required
		type : Required
	}
	service_account_tmpl : Template {
		automount_service_account_token : Required
		name : Required
		namespace : Required
	}
	service_tmpl : Template {
		cluster_ip : Required
		external_name : Required
		external_traffic_policy : Required
		health_Check_node_port : Required
		load_balancer_ip : Required
		loadbalancer_ingresses : Required
		name : Required
		namespace : Required
		publish_non_ready_addresses : Required
		session_affinity : Required
		type : Required
	}
	service_ingress_tmpl : Template {
		hostname : Required
		ip : Required
	}
	stateful_set_tmpl : Template {
		conditions : Required
		name : Required
		namespace : Required
		{{{ \{kubernetes.persistent_volume_claim_tmpl} }}}
		persistent_volume_claim : Required
		pod_management_policy : Required
		replicas : Required
		revision_history_limit : Required
		service_name : Required
		status_collision_count : Required
		status_current_revision : Required
		status_observed_generation : Required
		status_ready_replicas : Required
		status_replicas : Required
		status_updated_replicas : Required
		status_updated_revision : Required
	}
	storage_classes_tmpl : Template {
		allow_volume_expansion : Required
		allowed_toplogies : Required
		mount_options : Required
		name : Required
		namespace : Required
		provisioner : Required
		reclaim_policy : Required
		volume_binding_mode : Required
	}
	subject_tmpl : Template {
		api_group : Required
		kind : Required
		name : Required
		namespace : Required
	}
	subset_tmpl : Template {
		{{{ \{subset_port_tmpl} }}}
		ports : Required
		{{{ \{subset_address_tmpl} }}}
		addresses : Required
	}
	subset_address_tmpl : Template {
		ip : Required
		nodename : Required
		hostname : Required
	}
	subset_port_tmpl : Template {
		port : Required
		protocol : Required
	}
}
