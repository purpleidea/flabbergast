Introduction{{{Allows access to a Kubernetes cluster.}}}

{{{Selectors to restrict operations by labels}}}
label_selector : {
	{{{Combine existing label selectors.}}}
	in : Template (From native:cncf/kubernetes.label_in) {
		{{{The selectors to combine.}}}
		args : Used
	}
	{{{Checks that the label on the Kubernetes object is in the provided set of values.}}}
	in : Template (From native:cncf/kubernetes.label_in) {
		{{{The values the label can be.}}}
		args : Used
		{{{The name of the label.}}}
		key : Used
	}
	{{{Checks that the label on the Kubernetes object is not in the provided set of values.}}}
	not_in : Template (From native:cncf/kubernetes.label_not_in) {
		{{{The values the label can be.}}}
		args : Used
		{{{The name of the label.}}}
		key : Used
	}
	{{{Checks that the Kubernets object has a label with the provided name. The value of the label on the object is irrelevant.}}}
	with : Template (From native:cncf/kubernetes.label_with) {
		{{{The name of the label.}}}
		key : Used
	}
	{{{Checks that the Kubernetes object does not have a label with the provided name.}}}
	without : Template (From native:cncf/kubernetes.label_without) {
		{{{The name of the label.}}}
		key : Used
	}
}

{{{The default selection is no label restrictions.}}}
label_selectors : Null
{{{The default namespace to search is all of them.}}}
namespace : Null

