            <xsl:when test="@o_0:section = 8">System administration commands and daemons</xsl:when>
            <xsl:otherwise>Unknown</xsl:otherwise>
          </xsl:choose>
        </xsl:attribute>
        <xsl:value-of select="@o_0:section"/>
      </td>
      <td>
        <a href="{concat(@o_0:name, '.', @o_0:section, '.html')}">
          <xsl:value-of select="@o_0:name"/>
        </a>
      </td>
      <td>
        <xsl:value-of select="@o_0:description"/>
      </td>
      <td>
        <a href="{concat(@o_0:name, '.', @o_0:section, '.pdf')}">PDF</a>
      </td>
    </tr>
  </xsl:template>
  <xsl:template match="o_0:ref_link">
    <tr>
      <td>
        <a href="{concat('doc-', translate(@o_0:name, '/', '-'), '.xml')}">lib:<xsl:value-of select="@o_0:name"/></a>
      </td>
      <td>
        <xsl:copy-of select="*|text()"/>
      </td>
    </tr>
  </xsl:template>
  <xsl:template match="/o_0:versions">
    <html>
      <head>
        <meta http-equiv="content-type" content="text/xml; charset=utf-8">
          <!---->
        </meta>
        <title><xsl:value-of select="@o_0:name"/> Documentation</title>
        <link href="https://fonts.googleapis.com/css?family=Quicksand" rel="stylesheet">
          <!---->
        </link>
        <link rel="icon" href="/icon16.png" type="image/png" sizes="16x16">
          <!---->
        </link>
        <link rel="icon" href="/icon32.png" type="image/png" sizes="32x32">
          <!---->
        </link>
        <link rel="icon" href="/icon64.png" type="image/png" sizes="64x64">
          <!---->
        </link>
        <link rel="stylesheet" type="text/css" href="o_0.css">
          <!---->
        </link>
      </head>
      <body>
        <h1><a href="http://flabbergast.org/">Flabbergast</a><xsl:value-of select="@o_0:name"/> Documentation</h1>
        <div id="container">
          <div id="indexcontent">
            <xsl:apply-templates/>
            <table>
              <thead>
                <tr>
                  <th>Version</th>
                  <th>Source</th>
                </tr>
              </thead>
              <tbody>
                <xsl:for-each select="o_0:version">
                  <xsl:sort select="@o_0:id"/>
                  <tr>
                    <td>
                      <a href="{@o_0:tag}">
                        <xsl:value-of select="@o_0:name"/>
                      </a>
                    </td>
                    <td>
                      <a href="{concat(/o_0:versions/o_0:github, '/tree/', @o_0:tag)}">GitHub</a>
                    </td>
                  </tr>
                </xsl:for-each>
              </tbody>
            </table>
          </div>
        </div>
      </body>
    </html>
  </xsl:template>
  <xsl:template name="usefulPath">
    <xsl:param name="string"/>
    <xsl:choose>
      <xsl:when test="contains($string, ' ')">
        <xsl:call-template name="usefulPath">
          <xsl:with-param name="string" select="substring-after($string, ' ')"/>
        </xsl:call-template>
      </xsl:when>
      <xsl:otherwise>
        <xsl:value-of select="$string"/>
      </xsl:otherwise>
    </xsl:choose>
  </xsl:template>
</xsl:stylesheet>
